      </div>\n    </section>\n    <section id=\"contact\" class=\"mb-5\">\n        <div class=\"text-center section-title\">\n            <h2>Contact Us</h2>\n        </div>\n        <form id=\"contactForm\">\n            <div class=\"form-row\">\n                <div class=\"form-group col-md-6\">\n                    <label for=\"inputName\">Name</label>\n                    <input type=\"text\" class=\"form-control\" id=\"inputName\" required=\"\">\n                </div>\n                <div class=\"form-group col-md-6\">\n                    <label for=\"inputEmail\">Email</label>\n                    <input type=\"email\" class=\"form-control\" id=\"inputEmail\" required=\"\">\n                </div>\n            </div>\n            <div class=\"form-group\">\n                <label for=\"inputMessage\">Message</label>\n                <textarea class=\"form-control\" id=\"inputMessage\" rows=\"4\" required=\"\"></textarea>\n            </div>\n            <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\n        </form>\n    </section>\n</div>\n<div class=\"modal fade\" id=\"loginModal\" tabindex=\"-1\" aria-labelledby=\"loginModalLabel\" aria-hidden=\"true\">\n    <div class=\"modal-dialog\">\n        <div class=\"modal-content\">\n            <div class=\"modal-header\">\n                <h5 class=\"modal-title\" id=\"loginModalLabel\">Login</h5>\n                <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n                    <span aria-hidden=\"true\">×</span>\n                </button>\n            </div>\n            <div class=\"modal-body\">\n                <form id=\"loginForm\">\n                    <div class=\"form-group\">\n                        <label for=\"username\">Username</label>\n                        <input type=\"text\" class=\"form-control\" id=\"username\" required=\"\">\n                    </div>\n                    <div class=\"form-group\">\n                        <label for=\"password\">Password</label>\n                        <input type=\"password\" class=\"form-control\" id=\"password\" required=\"\">\n                    </div>\n                    <button type=\"submit\" class=\"btn btn-primary\">Login</button>\n                </form>\n            </div>\n        </div>\n    </div>\n</div>\n<footer class=\"bg-dark text-white text-center py-3\">\n    <p>© Stock Trading Guru. Created using ROBO B &amp; D</p>\n    <p>\n        <a href=\"https://www.facebook.com/nepalusa112/\" class=\"text-white mx-2\">Facebook</a>\n        <a href=\"https://x.com/i/flow/login?redirect_after_login=%2FNepalUSA112\" class=\"text-white mx-2\">Twitter</a>\n        <a href=\"https://www.youtube.com/channel/UCiikqjRhXSpmaJU0SdbrM1w\" class=\"text-white mx-2\">YouTube</a>\n        <a href=\"https://www.linkedin.com/company/nepal-usa/?viewAsMember=true\" class=\"text-white mx-2\">LinkedIn</a>\n    </p>\n</footer>\n<script src=\"https://code.jquery.com/jquery-3.5.1.min.js\"></script>\n<script src=\"https://cdn.jsdelivr.net/npm/@popperjs/core@2.5.2/dist/umd/popper.min.js\"></script>\n<script src=\"https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js\"></script>\n<script>\n    $('#contactForm').on('submit', function (e) {\n        e.preventDefault();\n        const name = $('#inputName').val();\n        const email = $('#inputEmail').val();\n        const message = $('#inputMessage').val();\n\n        $.ajax({\n            url: 'https://api.nepalusa.org/email-user',\n            type: 'POST',\n            contentType: 'application/json',\n            data: JSON.stringify({ name: name, email: email, message: message }),\n            success: function (response) {\n                alert('Message sent successfully!');\n                location.reload();\n            },\n            error: function () {\n                alert('Error in sending message');\n            }\n        });\n    });\n\n    function showLoginForm() {\n        $('#loginModal').modal('show');\n    }\n\n    $('#loginForm').on('submit', function (e) {\n        e.preventDefault();\n        const username = $('#username').val();\n        const password = $('#password').val();\n        if (username === 'test' && password === 'test') {\n            alert('You got it');\n            $('#loginModal').modal('hide');\n        } else {\n            alert('Invalid userid and password');\n        }\n    });\n\n    function fetchStockPrice(symbol, elementId) {\n        const url = `https://query1.finance.yahoo.com/v8/finance/chart/${symbol}`;\n        $.getJSON(url, function (data) {\n            const price = data.chart.result[0].meta.regularMarketPrice;\n            $(`#${elementId}`).text(price);\n        }).fail(function () {\n            $(`#${elementId}`).text(\"Failed to load\");\n        });\n    }\n\n    $(document).ready(function () {\n        fetchStockPrice('^GSPC', 'sp500');\n        fetchStockPrice('^DJI', 'dow');\n        fetchStockPrice('^IXIC', 'nasdaq');\n        fetchStockPrice('BTC-USD', 'bitcoin');\n        fetchStockPrice('GC=F', 'gold');\n    });\n</script>"
