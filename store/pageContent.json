  "page": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Kidos Fun Place</title>\n    <link rel=\"stylesheet\" href=\"https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css\">\n    <style>\n        .gallery img {\n            max-height: 200px;\n        }\n        .card-img-top {\n            max-height: 200px;\n            object-fit: cover;\n        }\n        #about img {\n            width: 400px;\n            height: 400px;\n            object-fit: cover;\n        }\n        #testimonials img {\n            max-height: 300px;\n            object-fit: cover;\n        }\n        #about .content {\n            background: #f9f9f9;\n            padding: 20px;\n            border-radius: 5px;\n        }\n        #about h2 {\n            font-family: 'Arial', sans-serif;\n        }\n        #about p {\n            font-size: 1.1em;\n            line-height: 1.6;\n        }\n        #about .highlight {\n            background: #ff0;\n            padding: 5px;\n            font-weight: bold;\n        }\n        #about .more-info {\n            margin-top: 20px;\n        }\n        #about .more-info p {\n            font-style: italic;\n        }\n    </style>\n</head>\n<body>\n    <nav class=\"navbar navbar-expand-lg navbar-light bg-light\">\n        <a class=\"navbar-brand\" href=\"#\">\n            <img src=\"https://raw.githubusercontent.com/BBBL-LLC/nepalusa_settings/master/Image_Urls/AI_GENERATED_IMG/B%26D_AI_1727640100987.jpeg\" alt=\"Logo\" style=\"height: 50px;\">\n            <span>The Fun Land</span>\n        </a>\n        <button class=\"navbar-toggler\" type=\"button\" data-toggle=\"collapse\" data-target=\"#navbarNav\" aria-controls=\"navbarNav\" aria-expanded=\"true\" aria-label=\"Toggle navigation\">\n            <span class=\"navbar-toggler-icon\"></span>\n        </button>\n        <div class=\"navbar-collapse collapse show\" id=\"navbarNav\" style=\"\">\n            <ul class=\"navbar-nav ml-auto\">\n                <li class=\"nav-item\"><a class=\"nav-link\" href=\"#about\">About</a></li>\n                <li class=\"nav-item\"><a class=\"nav-link\" href=\"#services\">Services</a></li>\n                <li class=\"nav-item\"><a class=\"nav-link\" href=\"#contact\">Contact</a></li>\n                <li class=\"nav-item\"><a class=\"nav-link btn btn-primary text-white\" href=\"#\" data-toggle=\"modal\" data-target=\"#loginModal\">Login</a></li>\n            </ul>\n        </div>\n    </nav>\n    <section id=\"about\" class=\"container mt-5\">\n        <h2>About Us</h2>\n        <div class=\"row\">\n            <div class=\"col-md-6\">\n                <img src=\"https://raw.githubusercontent.com/BBBL-LLC/nepalusa_settings/master/Image_Urls/AI_GENERATED_IMG/B%26D_AI_1727454056626.jpeg\" class=\"img-fluid\" alt=\"About Us\">\n            </div>\n            <div class=\"col-md-6 content\">\n                <p><span class=\"highlight\">Kidos Fun Place</span> is a platform where children can enjoy a variety of educational games, activities, and resources that make learning fun and engaging.</p>\n                <p>Our mission is to provide a safe and enjoyable environment for kids to discover and explore new learning opportunities.</p>\n                <div class=\"more-info\">\n                    <p>Our team is dedicated to continually improving and providing the best educational experiences for children of all ages.</p>\n                    <p>We believe that <strong>fun and education</strong> should go hand in hand, and we strive to make every moment at Kidos Fun Place a memorable one.</p>\n                </div>\n            </div>\n        </div>\n    </section>\n    <section id=\"services\" class=\"container mt-5\">\n        <h2>Our Services</h2>\n        <div class=\"row\">\n            <div class=\"col-md-4\">\n                <div class=\"card\">\n                    <img src=\"https://raw.githubusercontent.com/BBBL-LLC/nepalusa_settings/master/Image_Urls/AI_GENERATED_IMG/B%26D_AI_1727454309053.jpeg\" class=\"card-img-top\" alt=\"Service 1\">\n                    <div class=\"card-body\">\n                        <h5 class=\"card-title\">Educational Games</h5>\n                        <p class=\"card-text\">Fun and interactive games that teach various subjects.</p>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-md-4\">\n                <div class=\"card\">\n                    <img src=\"https://raw.githubusercontent.com/BBBL-LLC/nepalusa_settings/master/Image_Urls/AI_GENERATED_IMG/B%26D_AI_1727454990128.jpeg\" class=\"card-img-top\" alt=\"Service 2\">\n                    <div class=\"card-body\">\n                        <h5 class=\"card-title\">Creative Activities</h5>\n                        <p class=\"card-text\">Activities that inspire creativity and imagination.</p>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-md-4\">\n                <div class=\"card\">\n                    <img src=\"https://raw.githubusercontent.com/BBBL-LLC/nepalusa_settings/master/Image_Urls/AI_GENERATED_IMG/B%26D_AI_1727455278929.jpeg\" class=\"card-img-top\" alt=\"Service 3\">\n                    <div class=\"card-body\">\n                        <h5 class=\"card-title\">Learning Resources</h5>\n                        <p class=\"card-text\">A variety of resources to support your child's learning.</p>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </section>\n    <section id=\"testimonials\" class=\"container mt-5\">\n        <h2>Testimonials</h2>\n        <div id=\"testimonialCarousel\" class=\"carousel slide\" data-ride=\"carousel\">\n            <div class=\"carousel-inner\">\n                <div class=\"carousel-item\">\n                    <div class=\"d-block w-100\">\n                        <img src=\"https://raw.githubusercontent.com/BBBL-LLC/nepalusa_settings/master/Image_Urls/AI_GENERATED_IMG/B%26D_AI_1727455639290.jpeg\" class=\"d-block w-100\" alt=\"Testimonial 1\">\n                    </div>\n                    <div class=\"carousel-caption\">\n                        <p>\"My kids love Kidos Fun Place! They are learning so much while having fun.\"</p>\n                        <small>- Parent</small>\n                    </div>\n                </div>\n                <div class=\"carousel-item\">\n                    <div class=\"d-block w-100\">\n                        <img src=\"https://raw.githubusercontent.com/BBBL-LLC/nepalusa_settings/master/Image_Urls/AI_GENERATED_IMG/B%26D_AI_1727455822360.jpeg\" class=\"d-block w-100\" alt=\"Testimonial 2\">\n                    </div>\n                    <div class=\"carousel-caption\">\n                        <p>\"A fantastic platform for educational games and activities.\"</p>\n                        <small>- Teacher</small>\n                    </div>\n                </div>\n                <div class=\"carousel-item active\">\n                    <div class=\"d-block w-100\">\n                        <img src=\"https://raw.githubusercontent.com/BBBL-LLC/nepalusa_settings/master/Image_Urls/AI_GENERATED_IMG/B%26D_AI_1727455977149.jpeg\" class=\"d-block w-100\" alt=\"Testimonial 3\">\n                    </div>\n                    <div class=\"carousel-caption\">\n                        <p>\"Highly recommend Kidos Fun Place for all parents.\"</p>\n                        <small>- Educator</small>\n                    </div>\n                </div>\n            </div>\n            <a class=\"carousel-control-prev\" href=\"#testimonialCarousel\" role=\"button\" data-slide=\"prev\">\n                <span class=\"carousel-control-prev-icon\" aria-hidden=\"true\"></span>\n                <span class=\"sr-only\">Previous</span>\n            </a>\n            <a class=\"carousel-control-next\" href=\"#testimonialCarousel\" role=\"button\" data-slide=\"next\">\n                <span class=\"carousel-control-next-icon\" aria-hidden=\"true\"></span>\n                <span class=\"sr-only\">Next</span>\n            </a>\n        </div>\n    </section>\n    <section id=\"gallery\" class=\"container mt-5 gallery\">\n        <h2>Gallery</h2>\n        <div class=\"row\" id=\"galleryContent\">\n            <div class=\"col-md-4\"><img src=\"https://raw.githubusercontent.com/BBBL-LLC/nepalusa_settings/master/Image_Urls/AI_GENERATED_IMG/B%26D_AI_1727480713699.jpeg\" class=\"img-thumbnail\" alt=\"Gallery Image 1\"></div>\n            <div class=\"col-md-4\"><img src=\"https://raw.githubusercontent.com/BBBL-LLC/nepalusa_settings/master/Image_Urls/AI_GENERATED_IMG/B%26D_AI_1727454056626.jpeg\" class=\"img-thumbnail\" alt=\"Gallery Image 2\"></div>\n            <div class=\"col-md-4\"><img src=\"https://raw.githubusercontent.com/BBBL-LLC/nepalusa_settings/master/Image_Urls/AI_GENERATED_IMG/B%26D_AI_1727454309053.jpeg\" class=\"img-thumbnail\" alt=\"Gallery Image 3\"></div>\n        </div>\n    </section>\n    <section id=\"contact\" class=\"container mt-5 mb-5\">\n        <h2>Contact Us</h2>\n        <form id=\"contactForm\">\n            <div class=\"form-group\">\n                <label for=\"name\">Name</label>\n                <input type=\"text\" class=\"form-control\" id=\"name\" required=\"\">\n            </div>\n            <div class=\"form-group\">\n                <label for=\"email\">Email</label>\n                <input type=\"email\" class=\"form-control\" id=\"email\" required=\"\">\n            </div>\n            <div class=\"form-group\">\n                <label for=\"message\">Message</label>\n                <textarea class=\"form-control\" id=\"message\" rows=\"3\" required=\"\"></textarea>\n            </div>\n            <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\n        </form>\n    </section>\n    <footer class=\"bg-dark text-white text-center py-3\">\n        <p>Created using ROBO B &amp; D</p>\n        <div>\n            <a href=\"#\" class=\"text-white mr-4\">Facebook</a>\n            <a href=\"#\" class=\"text-white mr-4\">Twitter</a>\n            <a href=\"#\" class=\"text-white\">Instagram</a>\n        </div>\n    </footer>\n    <div class=\"modal fade\" id=\"loginModal\" tabindex=\"-1\" aria-labelledby=\"loginModalLabel\" aria-hidden=\"true\" style=\"display: none;\">\n        <div class=\"modal-dialog\" role=\"document\">\n            <div class=\"modal-content\">\n                <div class=\"modal-header\">\n                    <h5 class=\"modal-title\" id=\"loginModalLabel\">Login</h5>\n                    <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n                        <span aria-hidden=\"true\">×</span>\n                    </button>\n                </div>\n                <div class=\"modal-body\">\n                    <form id=\"loginForm\">\n                        <div class=\"form-group\">\n                            <label for=\"userid\">User ID</label>\n                            <input type=\"text\" class=\"form-control\" id=\"userid\" required=\"\">\n                        </div>\n                        <div class=\"form-group\">\n                            <label for=\"password\">Password</label>\n                            <input type=\"password\" class=\"form-control\" id=\"password\" required=\"\">\n                        </div>\n                        <button type=\"submit\" class=\"btn btn-primary\">Login</button>\n                    </form>\n                </div>\n            </div>\n        </div>\n    </div>\n    <script src=\"https://code.jquery.com/jquery-3.5.1.min.js\"></script>\n    <script src=\"https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.6/dist/umd/popper.min.js\"></script>\n    <script src=\"https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js\"></script>\n    <script>\n        $('#contactForm').on('submit', function (event) {\n            event.preventDefault();\n            alert('Thank you for your message. We will get back to you shortly.');\n            $(this).trigger('reset');\n        });\n        $('#loginForm').on('submit', function (event) {\n            event.preventDefault();\n            var userid = $('#userid').val();\n            var password = $('#password').val();\n            if (userid === 'test' && password === 'test') {\n                alert('Login successful!');\n            } else {\n                alert('Failed to login');\n            }\n            $('#loginModal').modal('hide');\n        });\n        $(document).ready(function(){\n            $.ajax({\n                url: 'https://api.nepalusa.org/list-image',\n                method: 'GET',\n                success: function(data){\n                    var galleryContent = $('#galleryContent');\n                    galleryContent.empty();\n                    data.forEach(function(imageUrl){\n                        var col = $('<div class=\"col-md-4\">');\n                        var img = $('<img>').addClass('img-thumbnail').attr('src', imageUrl);\n                        col.append(img);\n                        galleryContent.append(col);\n                    });\n                },\n                error: function(error){\n                    console.error('Failed to fetch gallery images:', error);\n                }\n            });\n        });\n    </script>\n</body>\n</html>"
}