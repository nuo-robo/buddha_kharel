izers -->\n\n            <!-- Start of Menu Items - Main Courses -->\n            <div class=\"col-lg-4\">\n                <h3>Main Courses</h3>\n                <div class=\"menu-item\">\n                    <img src=\"https://source.unsplash.com/1000x1000/?meal\" class=\"img-fluid\" alt=\"Main Course Image\">\n                    <h4>Dal Bhat</h4>\n                    <p>A classic Nepali meal with lentil soup, rice, and vegetables.</p>\n                    <p>Price: $15</p>\n                </div>\n                <!-- Add more main course items here -->\n            </div>\n            <!-- End of Menu Items - Main Courses -->\n\n            <!-- Start of Menu Items - Desserts -->\n            <div class=\"col-lg-4\">\n                <h3>Desserts</h3>\n                <div class=\"menu-item\">\n                    <img src=\"https://source.unsplash.com/1000x1000/?dessert\" class=\"img-fluid\" alt=\"Dessert Image\">\n                    <h4>Gulab Jamun</h4>\n                    <p>Indian dessert made of milk solids soaked in sugar syrup.</p>\n                    <p>Price: $8</p>\n                </div>\n                <!-- Add more dessert items here -->\n            </div>\n            <!-- End of Menu Items - Desserts -->\n        </div>\n    </div>\n</section>\n\n<!-- Reservations Section -->\n<section id=\"reservations\" class=\"reservations\">\n    <div class=\"container\">\n        <h2>Make a Reservation</h2>\n        <p>Book your table now to experience the best of Nepali cuisine.</p>\n        <form>\n            <label for=\"date\">Select Date:</label>\n            <input type=\"date\" id=\"date\" name=\"date\" required=\"\">\n            <label for=\"time\">Select Time:</label>\n            <input type=\"time\" id=\"time\" name=\"time\" required=\"\">\n            <label for=\"guests\">Number of Guests:</label>\n            <input type=\"number\" id=\"guests\" name=\"guests\" required=\"\">\n            <label for=\"special-requests\">Special Requests: